            <string>///Draw Skip hint when soace is pressed
if(keyboard_check(vk_space)) {
    var currentViewX = view_xview[view_current];
    var currentViewY = view_yview[view_current];
    var currentViewXSize = view_wview[view_current];
    var currentViewYSize = view_hview[view_current];
    
    
    var scale = currentViewXSize / room_width;
    var xPos = currentViewX + currentViewXSize / 2;
    var yPos = currentViewY + currentViewYSize - 80 * scale;
    
    
    draw_set_font(fontMain);
    draw_set_colour(c_white);
    draw_set_halign(fa_center);
    draw_set_valign(fa_middle);
    
//    draw_text(xPos, yPos, "Hold Space to skip.");
