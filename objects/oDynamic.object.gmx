            <string>///collisions with static objects

//horizontal collisions
if(place_meeting(x+hsp,y,oStatic)&amp;&amp;!goingToNextLevel)
{
    //bounce horizontally from walls.
    if(state=moving|| (place_meeting(x+hsp,y,oWall) &amp;&amp; state == falling) )state=idle;
    
    
    while(!place_meeting(x+sign(hsp),y,oStatic))
    {
        x+=sign(hsp);
    }
    hsp=0;
    
}
x+=hsp;

//vertical collisions
if(place_meeting(x,y+vsp,oStatic)&amp;&amp;!goingToNextLevel)
{
    if(vsp&lt;0)
    {
        state=deadCeiling;
    }

    while(!place_meeting(x,y+sign(vsp),oStatic))
    {
        y+=sign(vsp);
    }
        
    vsp=0;
    if(state=falling)state=moving;
}

y+=vsp;
