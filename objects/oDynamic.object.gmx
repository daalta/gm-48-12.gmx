            <string>///Move object and collide


//launch
if(global.lunch &amp;&amp; !statToHeavyToJump) 
{
    leftCollision=collision_point(bbox_left,bbox_bottom+1,oPlatformJump,0,0);
    rightCollision=collision_point(bbox_right,bbox_bottom+1,oPlatformJump,0,0);
    if(rightCollision!=noone||leftCollision!=noone)
    {
        vsp = LAUNCHSPEED;
        state = falling;
    }
}

//gravity
vsp += GRAVITY;

//Collide with ground
if(vsp &gt; 0) 
{
    //unten kollision mit boden
    collider=place_meeting(x, y + vsp, oStatic);
    if(collider != 0) 
    {
        vsp = 0; //Because we collided with the ground!
        if(state == falling) {
            state = idle;
        }
        //Move until ground is perfectly touched
        while(place_meeting(x, y + vsp + 1, oStatic) == 0)
        {
            vsp++;
        }
    }
}
//collide with ceiling
if(vsp &lt; 0) {
    if(collision_line(bbox_left, bbox_top, bbox_right, bbox_top, oStatic, false, false))
    {
        state = dead;
        instance_destroy();
    }
}


//Move
x += hsp;
y+=vsp;
</string>
