    if(collider.vsp &gt; 0) {
        if(collider.bbox_bottom + ENEMY_COLLISION_THRESHOLD &lt; bbox_bottom) {
            if(!statImmuneToPlayer) {
                state = dead;
                instance_destroy();
            }
            if(statDamagesPlayerOnHit &amp;&amp; !isJumpedOn) {
                collider.lives--;
            }
            isJumpedOn = 10;
            collider.vsp = ENEMY_BOUNCE_BACK;
        }
    }
}

//Kill player
if(state != dead &amp;&amp; !isJumpedOn &amp;&amp; !statHarmless) {
var collider2 = instance_place(x, y, oPlayer);
    if(collider2 != noone) {
        if(collider2.bbox_bottom - ENEMY_COLLISION_THRESHOLD * 2 &gt; bbox_top) {
            collider2.state = dead;
            //TODO death
            game_restart();
        }
    }
}
